{
  "manifest_version": 3,
  "name": "CRM Map Overlay",
  "version": "1.0",
  "description": "Overlay CRM data on Google Maps.",
  "permissions": ["activeTab", "storage"],
  "host_permissions": ["https://your-api-endpoint.com/*"],
  "content_scripts": [
    {
      "matches": ["https://your-crm-url.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}
